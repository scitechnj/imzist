
@section styles
{
    <link rel="stylesheet" href="../../Content/Login.css"/>
}

<div class="container" style="margin-top: 100px;">
    <div class="row">
        <div class="col-sm-6 col-lg-6 col-lg-offset-3">
            <div class="account-wall" style="border-radius: 15px; padding-left: 15px; padding-right: 15px;">
                <img class="profile-img" src="https://lh5.googleusercontent.com/-b0-k99FZlyE/AAAAAAAAAAI/AAAAAAAAAAA/eu7opA4byxI/photo.jpg?sz=120"
                     alt="">
                <form class="form-signin" method="POST">
                    <div class="form-group">
                        <input id="email" class="form-control" type="email" name="username" placeholder="valid email" >
                    </div>
                    <div class="form-group">
                        <input id="pass" class="form-control" type="password" name="password" placeholder="password" >
                    </div>
                    <div class="form-group">
                        <input id="conf" class="form-control" type="password" placeholder="confirm password" style="margin-bottom: 25px;" disabled="disabled" >
                    </div>
                        <div id="submit" class="btn btn-lg btn-primary btn-block">
                            Submit</div>
                </form>
            </div>
        </div>
    </div>
</div>

@section scripts
{
    <script type="text/javascript" src="/Scripts/UserScripts/signup.js"></script>

    <script>
        $(function () {
            var lis = $('.nav > li');
            menu_focus(lis[2], 4);
        })
    </script>
}


