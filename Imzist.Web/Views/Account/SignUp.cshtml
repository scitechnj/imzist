<div class="container" style="margin-top: 200px;">
    <div class="row">
        <div class="col-md-12">
            <form method="POST" class="form-horizontal" role="form">
                <div class="form-group">
                    <input id="email" class="form-control" type="email" name="username" placeholder="email" >
                </div>
                <div class="form-group">
                    <input id="pass" class="form-control" type="password" name="password" placeholder="password" >
                </div>
                <div class="form-group">
                    <input id="conf" class="form-control" type="password" placeholder="confirm password" disabled="disabled" >
                </div>
                <div class="form-group">
                    <input id="submit" type="submit" class="btn btn-primary" disabled="disabled" value="Submit" />
                </div>
            </form>
        </div>
    </div>
</div>

@section scripts
{
    <script type="text/javascript" src="/Scripts/UserScripts/signup.js"></script>
}


